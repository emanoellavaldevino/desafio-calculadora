console.log(rankeados())

function getWin(vitoria, derrota){
	let somatorio = vitoria - derrota
    return somatorio
    }
    
function rankeados(){
  	let resultado = getWin(100, 10)
    let nivel = " "
    
    if (resultado <= 10){
    	nivel = "Ferro"
    	
       } else if(resultado <= 20){
        	nivel = "Bronze"

	   } else if(resultado <= 50){
       	 	nivel = "Prata" 
       
       } else if(resultado <= 80){
       		nivel = "Ouro"
        
       } else if(resultado <= 90){
       		nivel = "Diamante"
        
       } else if(resultado <= 100){
       		nivel = "Lendário"
            
       } else if (resultado >= 101){
       		nivel = "Importal"
            
       } return `A heroina está com saldo de ${resultado} e está com o nivel de ${nivel}`
       
       }
            
            
       
